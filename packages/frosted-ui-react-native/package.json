{
  "name": "@frosted-ui/react-native",
  "version": "0.0.0",
  "main": "expo-router/entry",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "lucide-react-native": ">=0.300.0",
