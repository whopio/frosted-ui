{
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "module": "esnext",
    "sourceMap": true,
    "outDir": "dist",
    "target": "es5",
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "*": [
        "node_modules/*",
        "src/types/*"
      ]
    }
  }
}