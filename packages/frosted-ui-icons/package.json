{
  "name": "@frosted-ui/icons",
  "private": false,
  "version": "0.0.1-canary.0",
  "license": "MIT",
  "description": "Frosted UI React Icons",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "files": [
    "dist/**",
    "index.js",
    "manifest.json"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate-src": "generate-icon-lib --file=fxRLy35tq3pm2zNy5lAFfL && mv ./icons/default/* ./icons && rmdir ./icons/default && cd ./icons && zip -r ../frosted-ui-icons.zip ./",
    "clean": "rimraf dist",
    "build": "pnpm clean && pnpm build:js",
    "build:js": "pnpm build:js:cjs && pnpm build:js:esm",
    "build:js:cjs": "tsc --project tsconfig-cjs.json",
    "build:js:esm": "tsc --project tsconfig-esm.json",
    "prepublishOnly": "pnpm run build",
    "prepare": "pnpm run build"
  },
  "peerDependencies": {
    "@types/react": "*",
    "react": "^16.x || ^17.x || ^18.x"
  },
  "devDependencies": {
    "@types/react": "^16.8.17",
    "generate-icon-lib": "*",
    "react": "17.0.2",
    "rimraf": "^5.0.5",
    "typescript": "^4.5.3"
  }
}
