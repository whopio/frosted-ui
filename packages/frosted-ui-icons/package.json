{
  "name": "@frosted-ui/icons",
  "private": false,
  "version": "0.0.1-canary.23",
  "license": "MIT",
  "description": "Frosted UI React Icons",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/cjs/*.d.ts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    }
  },
  "files": [
    "dist/**",
    "index.js",
    "manifest.json"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/whopio/frosted-ui.git"
  },
  "homepage": "https://github.com/whopio/frosted-ui",
  "bugs": {
    "url": "https://github.com/whopio/frosted-ui/issues"
  },
  "scripts": {
    "generate-src": "generate-icon-lib --file=Qz3t7Yi565N2u4l3Ha1w99 && cd ./icons && zip -r ../frosted-ui-icons.zip ./",
    "clean": "rimraf dist",
    "build": "pnpm clean && pnpm build:js",
    "build:js": "pnpm build:js:cjs && pnpm build:js:esm",
    "build:js:cjs": "tsc --project tsconfig-cjs.json",
    "build:js:esm": "tsc --project tsconfig-esm.json",
    "prepublishOnly": "pnpm run build",
    "release": "turbo-module publish"
  },
  "peerDependencies": {
    "@types/react": "*",
    "react": "^16.x || ^17.x || ^18.x"
  },
  "devDependencies": {
    "@types/react": "^16.8.17",
    "generate-icon-lib": "workspace:*",
    "react": "17.0.2",
    "rimraf": "^5.0.5",
    "typescript": "^4.5.3"
  }
}
