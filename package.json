{
  "name": "turbo-module-monorepo",
  "version": "0.0.1-canary.98",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "pnpm": {
    "overrides": {
      "csstype": "3.1.3",
      "@types/react": "19.1.10",
      "@types/react-dom": "19.1.10"
    }
  },
  "scripts": {
    "build": "turbo run build",
    "build:storybook": "turbo run build-storybook",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.8",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "1.12.5"
  },
  "engines": {
    "node": "22.x",
    "pnpm": "^9.15.4"
  },
  "packageManager": "pnpm@9.15.4"
}
