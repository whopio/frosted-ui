{
  "name": "turbo-module-monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:storybook": "turbo run build-storybook",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.8",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "1.12.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.6.9"
}
